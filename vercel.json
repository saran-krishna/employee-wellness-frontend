{
  "version": 2,
  "name": "employee-wellness-frontend",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/([^/]+)/chat/([^/]+)/?$",
      "dest": "/welcome.html?company=$1&token=$2"
    },
    {
      "src": "/([^/]+)/chat/([^/]+)/session/?$",
      "dest": "/chat.html?company=$1&token=$2"
    },
    {
      "src": "/thank-you/?$",
      "dest": "/thank-you.html"
    },
    {
      "src": "/error/?$",
      "dest": "/error.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://web-production-fa83.up.railway.app"
  }
}
