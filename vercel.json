{
  "version": 2,
  "name": "employee-wellness-chat",
  "rewrites": [
    {
      "source": "/([^/]+)/chat/([^/]+)",
      "destination": "/index.html"
    },
    {
      "source": "/((?!static).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
